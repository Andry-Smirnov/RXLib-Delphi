#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = RtmRxCtl.bpl DclRxCtl.bpl RtmRxDB.bpl DclRxDB.bpl RtmRxBde.bpl \
  DclRxBde.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

RtmRxCtl.bpl: RtmRxCtl.dpk
  $(DCC)

DclRxCtl.bpl: DclRxCtl.dpk
  $(DCC)

RtmRxDB.bpl: RtmRxDB.dpk
  $(DCC)

DclRxDB.bpl: DclRxDB.dpk
  $(DCC)

RtmRxBde.bpl: RtmRxBde.dpk
  $(DCC)

DclRxBde.bpl: DclRxBde.dpk
  $(DCC)
